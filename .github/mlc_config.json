{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^https://127.0.0.1"
    },
    {
      "pattern": "^javascript:"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "docs/session-notes/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{GITHUB_WORKSPACE}}/"
    },
    {
      "pattern": "^docs/",
      "replacement": "{{GITHUB_WORKSPACE}}/docs/"
    },
    {
      "pattern": "^proposals/",
      "replacement": "{{GITHUB_WORKSPACE}}/proposals/"
    },
    {
      "pattern": "^reference/",
      "replacement": "{{GITHUB_WORKSPACE}}/reference/"
    },
    {
      "pattern": "^studies/",
      "replacement": "{{GITHUB_WORKSPACE}}/studies/"
    },
    {
      "pattern": "^discussions/",
      "replacement": "{{GITHUB_WORKSPACE}}/discussions/"
    }
  ],
  "httpStatusCodes": [200, 201, 202, 204, 206, 300, 301, 302, 303, 304, 307, 308],
  "timeout": "30s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHttpStatusCodes": [400, 401, 403, 404, 429, 500, 502, 503, 504],
  "aliveStatusCodes": [200, 206, 300, 301, 302, 303, 304, 307, 308]
}